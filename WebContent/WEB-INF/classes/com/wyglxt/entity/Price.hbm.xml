<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wyglxt.entity.Price" table="price">
		<id name="id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="gasPrice" type="double">
			<column name="gas_price" />
		</property>
		<property name="electricPrice" type="double">
			<column name="electric_price" />
		</property>
		<property name="waterPrice" type="double">
			<column name="water_price" />
		</property>
		<property name="wuyePrice" type="double">
			<column name="wuye_price" />
		</property>
		<property name="month" type="java.util.Date">
			<column name="month" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
