<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd
    http://www.springframework.org/schema/aop
    http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
    http://www.springframework.org/schema/tx
    http://www.springframework.org/schema/tx/spring-tx-3.0.xsd">

	<!-- service -->
	<bean id="houseService" class="com.wyglxt.service.impl.HouseServiceImpl"
		init-method="init" />
	<bean id="accountService" class="com.wyglxt.service.impl.AccountServiceImpl"
		init-method="init" />
	<bean id="ownerService" class="com.wyglxt.service.impl.OwnerServiceImpl"
		init-method="init" />
	<bean id="chargeService" class="com.wyglxt.service.impl.ChargeServiceImpl"
		init-method="init" />
	<bean id="priceService" class="com.wyglxt.service.impl.PriceServiceImpl"
		init-method="init" />
	<bean id="waterElecGasService" class="com.wyglxt.service.impl.WaterElecGasServiceImpl"
		init-method="init" />
	<bean id="announceService" class="com.wyglxt.service.impl.AnnounceServiceImpl"
		init-method="init" />
	<bean id="demoService" class="com.wyglxt.service.impl.DemoServiceImpl"
		init-method="init" />


</beans>