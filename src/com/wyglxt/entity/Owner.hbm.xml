<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wyglxt.entity.Owner" table="owner">
		<id name="id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		<property name="uuid" type="java.lang.String">
			<column name="uuid" length="50" not-null="true" />
		</property>
		<property name="houseNo" type="java.lang.String">
			<column name="house_no" length="50" not-null="true" />
		</property>
		<property name="name" type="java.lang.String">
			<column name="name" length="50" not-null="true" />
		</property>
		<property name="tel" type="java.lang.String">
			<column name="tel" length="50" not-null="true" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" length="50" not-null="true" />
		</property>
		<property name="population" type="java.lang.String">
			<column name="population" length="50" not-null="true" />
		</property>
		<property name="enterDate" type="java.util.Date">
			<column name="enter_date" length="50" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
